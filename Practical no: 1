 }


F) 
#include<stdio.h>
#include<conio.h>
#include<graphics.h>
#include<dos.h>
void main()
 {
 int gd,gm;
 detectgraph(&gd,&gm);
 
initgraph(&gd,&gm,”c:\\turboc3\\bgi”);
 line(60,60,100,130);
line(60,60,20,130);
line(20,130,100,130);
rectangle(20,130,100,250);
 line(60,60,250,60);
 line(250,60,280,130);
 rectangle(100,130,280,250);
 getch();
closegraph();
}






