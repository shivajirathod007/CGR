Practical no: 1
A)
#include<stdio.h>
#include<conio.h>
#include<graphics.h>
#include<dos.h>
void main()
 {
 int gd,gm;
 detectgraph(&gd,&gm);
 initgraph(&gd,&gm,"c:\\turboc3\\bgi");
 rectangle(40,40,180,150);
 rectangle(50,55,170,135);
 line(100,150,90,180);
 line(120,150,130,180);
 rectangle(30,180,200,230);
 getch();
 closegraph();
 }




