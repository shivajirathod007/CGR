C)
 
#include<stdio.h>
#include<conio.h>
#include<graphics.h>
#include<dos.h>
void main()
{
int gd,gm;
detectgraph(&gd,&gm);
intigraph(&gd,&gm,"c:\\turboc3\\bgi");
line(50,50,50,90);
line(50,50,130,50);
line(50,90,130,90);
line(130,50,130,20);
line(130,90,130,120);
line(130,20,200,65);
line(200,65,130,120);
getch();
closegraph();
}



D)
#include<stdio.h>
#include<conio.h>
#include<graphics.h>
#include<dos.h>
void main()
 {
 int gd,gm;
 detectgraph(&gd,&gm);
 initgraph(&gd,&gm,"c:\\turboc3\\bgi");
 circle(90,90,50);
 circle(70,70,10);
 circle(110,70,10);
 ellipse(90,189,100,80,35,50);
 line(125,180,190,220);
 line(55,180,-250,400);
 getch();
 closegraph();
 }



